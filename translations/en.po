#, no-wrap
msgid "{\n"
"  \"engineVersion\" : \"GLC:7.0|U:1.01374|eng:1.0\",\n"
"  \"numResults\" : 0, <1>\n"
"  \"executionTimems\" : 578.1462\n"
"}\n"
msgstr ""

#. type: Plain text
#: 400.es.adoc:177
msgid "Ningún resultado encontrado."
msgstr ""

#. type: Plain text
#: 400.es.adoc:179
#, no-wrap
msgid "*Caso 2)* Un único resultado encontrado. En la celda correspondiente verá, por ejemplo, lo siguiente:\n"
msgstr ""

#. type: delimited block -
#: 400.es.adoc:209
#, no-wrap
msgid "{\n"
"  \"engineVersion\": \"GLC:7.0|U:1.01374|eng:1.0\",\n"
"  \"numResults\": 1,\n"
"  \"executionTimems\": 484.3969,\n"
"  \"resultSet\": {\n"
"    \"type\": \"FeatureCollection\",\n"
"    \"features\": [\n"
"      {\n"
"        \"type\": \"Feature\",\n"
"        \"geometry\": {\n"
"          \"type\": \"Point\",\n"
"          \"coordinates\": [ -64.471941, -23.643418 ] <1>\n"
"        },\n"
"        \"properties\": {\n"
"          \"parsePattern\": \"YUTO\", <2>\n"
"          \"precision\": \"High\",\n"
"          \"score\": 79,\n"
"          \"uncertaintyRadiusMeters\": 3036, <3>\n"
"          \"uncertaintyPolygon\": \"Unavailable\", <4>\n"
"          \"displacedDistanceMiles\": 0, <5>\n"
"          \"displacedHeadingDegrees\": 0,\n"
"          \"debug\": \":GazPartMatch=False|:inAdm=True|:Adm=JUJUY|:NPExtent=5040|:NP=YUTO|:KFID=|YUTO\" <6>\n"
"        }\n"
"      }\n"
"    ],\n"
"    \"crs\": { \"type\": \"EPSG\", \"properties\": { \"code\": 4326 } }\n"
"  }\n"
"}\n"
msgstr ""

#. type: Plain text
#: 400.es.adoc:211
msgid "Las coordenadas: [source,javascript]`\"coordinates\": [-64.471941, -23.643418]`"
msgstr ""

#. type: Plain text
#: 400.es.adoc:212
